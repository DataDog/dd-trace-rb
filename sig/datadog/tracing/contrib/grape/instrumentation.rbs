module Datadog
  module Tracing
    module Contrib
      module Grape
        module Instrumentation
          def self.included: (untyped base) -> untyped
          module ClassMethods
            def generate_api_method: (*untyped params) ?{ () -> untyped } -> untyped
          end
          module InstanceMethods
            def run: (*untyped args) -> untyped
          end
        end
      end
    end
  end
end
