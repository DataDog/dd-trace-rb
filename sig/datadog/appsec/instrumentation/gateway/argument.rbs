module Datadog
  module AppSec
    module Instrumentation
      class Gateway
        class Argument
          def initialize: (*untyped) -> void
        end

        class User < Argument
          @id: String

          @login: String?

          attr_reader id: String

          attr_reader login: String?

          def initialize: (String id, String? login) -> void
        end
      end
    end
  end
end
