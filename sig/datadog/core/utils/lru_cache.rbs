module Datadog
  module Core
    module Utils
      class LRUCache
        extend ::Forwardable

        @store: ::Hash[untyped, untyped]

        @max_size: ::Integer

        def initialize: (::Integer max_size) -> void

        def []: (untyped key) -> untyped?

        def store: (untyped key, untyped value) -> untyped

        def fetch_or_store: (untyped key) { () -> untyped } -> untyped

        def clear: () -> void

        def empty?: () -> bool
      end
    end
  end
end


