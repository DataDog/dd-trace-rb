module Datadog
  module AIGuard
    module Evaluation
      class Result
        ALLOW_ACTION: ::String
        DENY_ACTION: ::String
        ABORT_ACTION: ::String

        attr_reader action: ::String
        attr_reader reason: ::String
        attr_reader tags: ::Array[::String]

        def initialize: (::Hash[::String, untyped] raw_response_body) -> void

        def allow?: () -> bool

        def deny?: () -> bool

        def abort?: () -> bool
      end
    end
  end
end
