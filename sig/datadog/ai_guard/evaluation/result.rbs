module Datadog
  module AIGuard
    module Evaluation
      class Result
        ALLOW_ACTION: ::String
        DENY_ACTION: ::String
        ABORT_ACTION: ::String

        attr_reader action: ::String
        attr_reader reason: ::String
        attr_reader tags: ::Array[::String]

        type raw_response_body = {
          'data' => {
            'attributes' => {
              'action' => ::String,
              'reason' => ::String,
              'tags' => ::Array[::String]
            }
          }
        }

        def initialize: (raw_response_body raw_response) -> void

        def allow?: () -> bool

        def deny?: () -> bool

        def abort?: () -> bool
      end
    end
  end
end
